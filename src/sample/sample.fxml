        <TextField fx:id="sName"/>
        <Label text="Student Age"/>
        <TextField fx:id="sAge"/>
        <Label text="Major"/>
        <TextField fx:id="sMajor"/>
        <Label text="GPA"/>
        <TextField prefWidth="10" fx:id="sGPA"/>


        <Label text="Students Age 20"/>
        <JFXButton text="Filter Age" prefWidth="200" prefHeight="40" fx:id="filterAge"/>
