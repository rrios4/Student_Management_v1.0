<HBox fx:controller="sample.Controller" xmlns:fx="http://javafx.com/fxml" alignment="center">

    <VBox spacing="5">
        <JFXListView prefHeight="500" prefWidth="100" fx:id="studentListView"/>
        <JFXButton text="Create Database" prefWidth="300" prefHeight="40" fx:id="createDB"/>
        <JFXButton text="LOAD" prefWidth="300" prefHeight="40" fx:id="loaddatabutton"/>
        <JFXButton text="DELETE" prefWidth="300" prefHeight="40" fx:id="deletetablebutton"/>

    </VBox>
    <VBox spacing="5">
        <Label text="Student ID"/>
        <TextField fx:id="sId"/>
